<form action="{{ path('_msgform') }}" method="post" {{ form_enctype(form) }}>

    {{ form_errors(form) }}

	<div>
	    {{ form_label(form.name, 'Enter your name') }}
	    {{ form_errors(form.name) }}
	    {{ form_widget(form.name) }}
	</div>

	<div>
	    {{ form_label(form.email, 'Enter e-mail address') }}
	    {{ form_errors(form.email) }}
	    {{ form_widget(form.email) }}
	</div>

	{{ form_rest(form) }}

    <input type="submit" />
</form>
